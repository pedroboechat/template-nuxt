{
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint:all": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:types",
    "lint:cache": "eslint . --cache --cache-location node_modules/.cache/eslint --format compact",
    "lint:ci": "pnpm lint:all",
    "lint:eslint": "eslint . --format compact",
    "lint:fix": "prettier --write . && eslint . --fix",
    "lint:prettier": "prettier --check .",
    "lint:staged:fix": "git diff --name-only --cached --relative -- '*.js' '*.ts' '*.vue' '*.json' '*.css' '*.scss' | xargs -r prettier --write && git diff --name-only --cached --relative -- '*.js' '*.ts' '*.vue' '*.json' '*.css' '*.scss' | xargs -r eslint --fix",
    "lint:staged": "git diff --name-only --cached --relative -- '*.js' '*.ts' '*.vue' '*.json' '*.css' '*.scss' | xargs -r eslint --max-warnings=0 --format compact",
    "lint:types": "vue-tsc --noEmit",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview"
  },
  "devDependencies": {
    "@nuxt/eslint": "0.5.7",
    "@nuxt/fonts": "0.8.0",
    "@nuxt/scripts": "0.9.2",
    "@nuxtjs/sitemap": "6.0.1",
    "@types/node": "22.5.5",
    "eslint": "9.10.0",
    "nuxt": "3.13.2",
    "prettier": "3.3.3",
    "typescript": "5.6.2",
    "vite-plugin-eslint2": "4.4.1",
    "vite-plugin-vuetify": "2.0.4",
    "vuetify": "3.7.1"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@pinia/nuxt": "0.5.4",
    "maska": "3.0.2",
    "sass": "1.78.0"
  }
}
